#!/bin/bash

# source the environment variables
# the env.sh script sets the ATTIVIO_HOME / ATTIVIO_PROJECT / ZOOKEEPER_HOST
. ./env.sh $1 $2

$ATTIVIO_HOME/bin/aie-exec deployproject -f $ATTIVIO_PROJECT/conf/iheartradio-<%= @attivio_env %>.xml -n iheartradio-<%= @attivio_env %> -z $ZOOKEEPER_HOST


