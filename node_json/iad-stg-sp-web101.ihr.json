      "pid_file": "/var/run/mysqld/mysqld.pid",
      "socket": "/var/lib/mysql/mysql.sock"
    },
    "nagios": {
      "nsca": {
        "authkey": "JZ_OtF3rF4ponomK0g5F"
      }
    },
    "java": {
      "oracle": {
        "accept_oracle_download_terms": true
      }
    },
    "application": {
      "webplayer": {
        "strategy": "deploy"
      }
    },
    "tags": [
      "webplayer-deployed"
    ],
    "platform?": "ubuntu",
    "apache": {
      "binary": "/usr/sbin/httpd.worker"
    },
    "deploy": {
      "webplayer": {
        "shallow_clone": "false"
      }
    },
    "snmp": {
