{
  "name": "iad-stg-sp-web101.ihr",
  "chef_environment": "stage",
  "run_list": [
    "role[base]",
    "role[webplayers]",
    "role[webplayer-membase]",
    "role[memcached]",
    "recipe[webplayer::memcached]"
